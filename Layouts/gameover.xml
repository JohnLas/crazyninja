<?xml version="1.0" encoding="utf-8" ?>
<c2layout>
    <name>gameover</name>
    <event-sheet>playground</event-sheet>
    <size>
        <width>960</width>
        <height>640</height>
    </size>
    <margins>
        <horizontal>500</horizontal>
        <vertical>500</vertical>
    </margins>
    <unbounded-scrolling>0</unbounded-scrolling>
    <layers>
        <layer name="Background">
            <initially-visible>1</initially-visible>
            <background-color>255,255,255</background-color>
            <transparent>0</transparent>
            <parallax>
                <x>1</x>
                <y>1</y>
            </parallax>
            <zoom-rate>1</zoom-rate>
            <opacity>1</opacity>
            <force-own-texture>0</force-own-texture>
            <instances>
                <instance type="Sky">
                    <properties>
                        <initial-visibility>Visible</initial-visibility>
                        <initial-frame>0</initial-frame>
                        <effect>(none)</effect>
                    </properties>
                    <world>
                        <x>480</x>
                        <y>320</y>
                        <z>0</z>
                        <width>960</width>
                        <height>640</height>
                        <depth>0</depth>
                        <hotspotX>0.5</hotspotX>
                        <hotspotY>0.5</hotspotY>
                        <angle>0</angle>
                        <opacity>1</opacity>
                    </world>
                </instance>
            </instances>
        </layer>
        <layer name="Layer 0">
            <initially-visible>1</initially-visible>
            <background-color>255,255,255</background-color>
            <transparent>1</transparent>
            <parallax>
                <x>1</x>
                <y>1</y>
            </parallax>
            <zoom-rate>1</zoom-rate>
            <opacity>1</opacity>
            <force-own-texture>0</force-own-texture>
            <instances>
                <instance type="GameOver">
                    <properties>
                        <text>GAME OVER</text>
                        <initial-visibility>Visible</initial-visibility>
                        <font>Tahoma,-96,400,0</font>
                        <color>0,51,153</color>
                        <horizontal-alignment>Center</horizontal-alignment>
                        <hotspot>Top-left</hotspot>
                        <wrapping>Word</wrapping>
                    </properties>
                    <world>
                        <x>174</x>
                        <y>117</y>
                        <z>0</z>
                        <width>600</width>
                        <height>120</height>
                        <depth>0</depth>
                        <hotspotX>0</hotspotX>
                        <hotspotY>0</hotspotY>
                        <angle>0</angle>
                        <opacity>1</opacity>
                    </world>
                </instance>
                <instance type="Text">
                    <properties>
                        <text>Touch screen to continue</text>
                        <initial-visibility>Visible</initial-visibility>
                        <font>Verdana,-48,400,1</font>
                        <color>0,153,204</color>
                        <horizontal-alignment>Left</horizontal-alignment>
                        <hotspot>Top-left</hotspot>
                        <wrapping>Word</wrapping>
                    </properties>
                    <world>
                        <x>173</x>
                        <y>497</y>
                        <z>0</z>
                        <width>644</width>
                        <height>78</height>
                        <depth>0</depth>
                        <hotspotX>0</hotspotX>
                        <hotspotY>0</hotspotY>
                        <angle>0</angle>
                        <opacity>1</opacity>
                    </world>
                </instance>
                <instance type="ScoreFlash">
                    <properties>
                        <text>Text</text>
                        <initial-visibility>Visible</initial-visibility>
                        <font>Tahoma,-133,400,0</font>
                        <color>0,0,0</color>
                        <horizontal-alignment>Center</horizontal-alignment>
                        <hotspot>Top-left</hotspot>
                        <wrapping>Word</wrapping>
                    </properties>
                    <behavior-instances>
                        <behavior-instance type="Fade">
                            <properties>
                                <active-at-start>Yes</active-at-start>
                                <fade-in-time>2</fade-in-time>
                                <wait-time>0</wait-time>
                                <fade-out-time>0</fade-out-time>
                                <destroy>After fade out</destroy>
                            </properties>
                        </behavior-instance>
                    </behavior-instances>
                    <world>
                        <x>26</x>
                        <y>264</y>
                        <z>0</z>
                        <width>908</width>
                        <height>178</height>
                        <depth>0</depth>
                        <hotspotX>0</hotspotX>
                        <hotspotY>0</hotspotY>
                        <angle>0</angle>
                        <opacity>1</opacity>
                    </world>
                </instance>
            </instances>
        </layer>
    </layers>
    <nonworld-instances />
</c2layout>
